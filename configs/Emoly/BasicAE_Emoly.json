{
    "input_shape": [16, 128, 128, 3],
    "data_format": "channels_last",
    "use_dense_embeddings": "False",
    "encoder":
    [
        {"filters": 16, "kernel_size": [5, 5, 5], "strides": 2, "depth": 1},
        {"filters": 32, "kernel_size": [5, 5, 5], "strides": 2, "depth": 1},
        {"filters": 64, "kernel_size": [4, 5, 5], "strides": 2, "depth": 1}
    ],
    "embeddings_layer" : {"filters": 128, "kernel_size": [2, 5, 5], "strides": 2, "depth": 1},
    "decoder":
    [
        {"filters": 64, "kernel_size": [2, 5, 5], "strides": 2, "depth": 1},
        {"filters": 32, "kernel_size": [4, 5, 5], "strides": 2, "depth": 1},
        {"filters": 16, "kernel_size": [5, 5, 5], "strides": 2, "depth": 1},
        {"filters": 8, "kernel_size": [5, 5, 5], "strides": 2, "depth": 1}
    ],
    "block_type": "conv_block",
    "pooling": "strides",
    "upsampling": "strides",
    "use_batch_normalization": "False",
    "use_spectral_norm": "False",
    "default_activation": {"name": "elu", "alpha": 0.2},
    "embeddings_activation": {"name": "linear"},
    "output_activation": {"name": "sigmoid"},
    "weights_initializer": {"mode": "fan_in", "scale": 1.0, "distribution": "normal"},
    "optimizer": {"name": "Adam", "lr": 1e-3, "beta_1": 0.9, "beta_2": 0.999, "decay": 0.0},
    "reconstruction_loss": "L2",
    "metrics": [],
    "lr_schedule": {"drop_epochs": 200, "drop_factor": 0.25}
}
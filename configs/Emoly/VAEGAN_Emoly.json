{
    "input_shape": [16, 128, 128, 3],
    "embeddings_size": 64,
    "use_dense_embeddings": "False",
    "encoder": [
        {"filters": 32, "kernel_size": 3, "strides": 2, "depth": 1},
        {"filters": 48, "kernel_size": 3, "strides": 2, "depth": 1},
        {"filters": 64, "kernel_size": 3, "strides": 2, "depth": 1}
    ],
    "decoder": [
        {"filters": 64, "kernel_size": 3, "strides": 2, "depth": 1},
        {"filters": 48, "kernel_size": 3, "strides": 2, "depth": 1},
        {"filters": 32, "kernel_size": 3, "strides": 2, "depth": 1}
    ],
    "discriminator": [
        {"filters": 32, "kernel_size": 3, "strides": 2, "depth": 1},
        {"filters": 32, "kernel_size": 3, "strides": 2, "depth": 1},
        {"filters": 32, "kernel_size": 3, "strides": 2, "depth": 1},
        {"filters": 32, "kernel_size": 3, "strides": 2, "depth": 1},
        {"filters": 32, "kernel_size": 3, "strides": 2, "depth": 1}
    ],
    "block_type": "conv_block",
    "pooling": "average",
    "upsampling": "upsampling",
    "use_batch_normalization": "True",
    "use_spectral_norm": "False",
    "default_activation": {"name": "leaky_relu", "alpha": 0.2},
    "embeddings_activation": {"name": "linear"},
    "output_activation": {"name": "tanh"},
    "weights_initializer": {"mode": "fan_in", "scale": 1.0, "distribution": "truncated_normal"},
    "optimizer": {"name": "Adam", "lr": 1e-3, "beta_1": 0.9, "beta_2": 0.99, "decay": 0.0},
    "reconstruction_loss": "L2",
    "metrics": {
        "discriminator": [],
        "autoencoder": ["mean_squared_error"],
        "generator": []
    },
    "data_generators": {
        "train": {
            "_blurring": {"max_sigma": 2.0, "kernel_size": [5, 5]},
            "_cropping": {"width_range": 0.25},
            "_brightness": {"bias": 0.2},
            "_dropout_rate": 0.05
        },
        "test": {
            "_dropout_rate": 0.0
        }
    },
    "losses": {"discriminator": "bce", "autoencoder": "L1", "generator": "bce"},
    "loss_weights": {"reconstruction": 1.0, "adversarial": 0.1, "divergence": 0.01},
    "lr_schedule": {"drop_epochs": 200, "drop_factor": 0.25},
    "discriminator_steps": 1
}
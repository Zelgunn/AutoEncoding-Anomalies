{
    "input_shape": [8, 128, 192, 1],
    "embeddings_size": 240,
    "embeddings_reshape": [1, 2, 3],
    "use_dense_embeddings": "False",
    "encoder": [
        {"filters": 16, "kernel_size": [3, 3, 3], "strides": [1, 2, 2], "depth": 8},
        {"filters": 16, "kernel_size": [3, 3, 3], "strides": [2, 2, 2], "depth": 8},
        {"filters": 32, "kernel_size": [2, 3, 3], "strides": [1, 2, 2], "depth": 8},
        {"filters": 32, "kernel_size": [2, 3, 3], "strides": [2, 2, 2], "depth": 8},
        {"filters": 64, "kernel_size": [1, 3, 3], "strides": [1, 2, 2], "depth": 8},
        {"filters": 64, "kernel_size": [1, 3, 3], "strides": [2, 2, 2], "depth": 8}
    ],
    "decoder": [
        {"filters": 64, "kernel_size": [1, 3, 3], "strides": [2, 2, 2], "depth": 8},
        {"filters": 64, "kernel_size": [1, 3, 3], "strides": [1, 2, 2], "depth": 8},
        {"filters": 32, "kernel_size": [2, 3, 3], "strides": [2, 2, 2], "depth": 8},
        {"filters": 32, "kernel_size": [2, 3, 3], "strides": [1, 2, 2], "depth": 8},
        {"filters": 16, "kernel_size": [3, 3, 3], "strides": [2, 2, 2], "depth": 8},
        {"filters": 16, "kernel_size": [3, 3, 3], "strides": [1, 2, 2], "depth": 8}
    ],
    "discriminator": [
        {"filters": 16, "kernel_size": [3, 3, 3], "strides": [1, 2, 2], "depth": 8},
        {"filters": 16, "kernel_size": [3, 3, 3], "strides": [2, 2, 2], "depth": 8},
        {"filters": 32, "kernel_size": [2, 3, 3], "strides": [1, 2, 2], "depth": 8},
        {"filters": 32, "kernel_size": [2, 3, 3], "strides": [2, 2, 2], "depth": 8},
        {"filters": 64, "kernel_size": [1, 3, 3], "strides": [1, 2, 2], "depth": 8},
        {"filters": 64, "kernel_size": [1, 3, 3], "strides": [2, 2, 2], "depth": 8}
    ],
    "block_type": "conv_block",
    "pooling": "average",
    "upsampling": "upsampling",
    "use_batch_normalization": "False",
    "use_spectral_norm": "False",
    "default_activation": {"name": "leaky_relu", "alpha": 0.2},

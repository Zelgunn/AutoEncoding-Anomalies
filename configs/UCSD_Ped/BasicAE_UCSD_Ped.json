{
    "input_shape": [16, 128, 128, 1],
    "embeddings_size": 64,
    "use_dense_embeddings": "False",
    "encoder":
    [
        {"filters": 32, "kernel_size": 3, "strides": 2, "depth": 1},
        {"filters": 48, "kernel_size": 3, "strides": 2, "depth": 1},
        {"filters": 64, "kernel_size": 3, "strides": 2, "depth": 1}
    ],
    "decoder":
    [
        {"filters": 48, "kernel_size": 3, "strides": 2, "depth": 1},
        {"filters": 32, "kernel_size": 3, "strides": 2, "depth": 1},
        {"filters": 32, "kernel_size": 3, "strides": 2, "depth": 1}
    ],
    "block_type": "conv_block",
    "pooling": "max",
    "upsampling": "strides",
    "use_batch_normalization": "True",
    "use_spectral_norm": "False",
    "default_activation": {"name": "leaky_relu", "alpha": 0.3},
    "embeddings_activation": {"name": "leaky_relu", "alpha": 0.3},
    "output_activation": {"name": "sigmoid"},
    "weights_initializer": {"mode": "fan_in", "scale": 1.0, "distribution": "normal"},
    "optimizer": {"name": "Adam", "lr": 1e-3, "beta_1": 0.9, "beta_2": 0.99, "decay": 0.0},
    "reconstruction_loss": "L2",
    "metrics": ["mean_squared_error"],
    "pixel_level_labels_size": [8, 16],
    "data_generators":
    {
        "train":
        {
            "_blurring" : {"max_sigma" : 5.0, "kernel_size" : [3, 3]},
            "_cropping" : {"width_range" : 0.75},
            "_brightness" : {"bias" : 0.5},
            "_dropout_rate": 0.05
        },
        "test":
        {
            "dropout_rate": 0.0
        }
    },
    "lr_schedule": {"drop_epochs": 200, "drop_factor": 0.25}
}
{
    "input_shape": [8, 192, 256, 1],
    "embeddings_size": 240,
    "embeddings_reshape": [1, 3, 4],
    "use_dense_embeddings": "False",
    "use_spectral_norm": "True",
    "use_batch_normalization_in_res_blocks": "False",
    "encoder": [
        {"filters": 16, "kernel_size": [3, 3, 3], "strides": [1, 2, 2], "resblock": "True"},
        {"filters": 16, "kernel_size": [3, 3, 3], "strides": [2, 2, 2], "resblock": "True"},
        {"filters": 32, "kernel_size": [2, 3, 3], "strides": [1, 2, 2], "resblock": "True"},
        {"filters": 32, "kernel_size": [2, 3, 3], "strides": [2, 2, 2], "resblock": "True"},
        {"filters": 64, "kernel_size": [1, 3, 3], "strides": [1, 2, 2], "resblock": "True"},
        {"filters": 64, "kernel_size": [1, 3, 3], "strides": [2, 2, 2], "resblock": "True"}
    ],
    "decoder": [
        {"filters": 64, "kernel_size": [1, 3, 3], "strides": [2, 2, 2], "resblock": "True"},
        {"filters": 64, "kernel_size": [1, 3, 3], "strides": [1, 2, 2], "resblock": "True", "dropout" : 0.5},
        {"filters": 32, "kernel_size": [2, 3, 3], "strides": [2, 2, 2], "resblock": "True", "dropout" : 0.5},
        {"filters": 32, "kernel_size": [2, 3, 3], "strides": [1, 2, 2], "resblock": "True", "dropout" : 0.5},
        {"filters": 16, "kernel_size": [3, 3, 3], "strides": [2, 2, 2], "resblock": "True", "dropout" : 0.5},
        {"filters": 16, "kernel_size": [3, 3, 3], "strides": [1, 2, 2], "resblock": "True"}
    ],
    "discriminator": [
        {"filters": 16, "kernel_size": [3, 3, 3], "strides": [1, 2, 2], "resblock": "True"},
        {"filters": 16, "kernel_size": [3, 3, 3], "strides": [2, 2, 2], "resblock": "True"},
        {"filters": 32, "kernel_size": [2, 3, 3], "strides": [1, 2, 2], "resblock": "True"},
        {"filters": 32, "kernel_size": [2, 3, 3], "strides": [2, 2, 2], "resblock": "True"},
        {"filters": 64, "kernel_size": [1, 3, 3], "strides": [1, 2, 2], "resblock": "True"},
        {"filters": 64, "kernel_size": [1, 3, 3], "strides": [2, 2, 2], "resblock": "True"}
    ],
    "pooling": "max",
    "default_activation": {"name": "leaky_relu", "alpha": 0.2},
    "embeddings_activation": {"name": "linear"},
    "output_activation": {"name": "tanh"},
    "weights_initializer": {"mode" : "fan_in", "scale" : 0.2, "distribution" : "normal"},
    "residual_scaling": "1.0",
    "optimizer": {"name": "adam", "lr": 2e-5, "beta_1": 0.5, "beta_2": 0.999, "decay": 0.0, "rho": 0.9},
    "losses": {"discriminator": "bce", "autoencoder": "L1", "generator": "bce"},
    "loss_weights": {"reconstruction": 100.0, "adversarial": 10.0, "divergence": 0.1},
    "metrics": {
        "discriminator": ["acc"],
        "autoencoder": ["mse"],
        "generator": ["acc"]
    },
    "discriminator_steps": 3,
    "pixel_level_labels_size": [16, 24],
    "data_generators": {
        "train": {"dropout_rate": 0.2},
        "test": {"dropout_rate": 0.0}
    }
}
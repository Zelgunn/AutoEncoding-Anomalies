{
    "input_shape": [192, 256, 1],
    "embeddings_size": 120,
    "embeddings_reshape": [3, 4],
    "use_dense_embeddings": "False",
    "use_resblock": "True",
    "use_spectral_norm": "True",
    "encoder": [
        {"filters": 32, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 32, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 64, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 64, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 96, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 128, "kernel_size": 3, "strides": 2, "padding": "same"}
    ],
    "decoder": [
        {"filters": 128, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 96, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 64, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 64, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 32, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 32, "kernel_size": 3, "strides": 2, "padding": "same"}
    ],
    "default_activation": {"name": "leaky_relu", "alpha": 0.2},
    "embeddings_activation": {"name": "linear"},
    "output_activation": {"name": "tanh"},
    "optimizer": {"name": "Adam", "lr": 1e-5, "beta_1": 0.0, "beta_2": 0.99, "decay": 1e-8},
    "decoder_steps": 2,
    "loss_weights": {
        "encoder": {
            "real_data": {"reconstruction": 10.0, "divergence": 1.0},
            "fake_data": {"reconstruction": 0.0, "divergence": -1.0}
        },
        "decoder": {
            "real_data": {"reconstruction": 0.0, "divergence": 1.0},
            "fake_data": {"reconstruction": 1000.0, "divergence": 1.0}
        }
    },
    "reconstruction_metrics": {"real": "L1", "fake": "cos"},
    "pixel_level_labels_size": [16, 24],
    "data_generators": {
        "train": {"dropout_rate": 0.2},
        "test": {"dropout_rate": 0.0}
    }
}
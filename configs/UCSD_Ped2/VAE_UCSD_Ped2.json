{
    "input_shape": [128, 192, 1],
    "embeddings_size": 240,
    "embeddings_reshape": [2, 3],
    "use_resblock": "True",
    "use_spectral_norm": "False",
    "encoder": [
        {"filters": 32, "kernel_size": 5, "strides": 2, "padding": "same"},
        {"filters": 32, "kernel_size": 5, "strides": 2, "padding": "same"},
        {"filters": 64, "kernel_size": 5, "strides": 2, "padding": "same"},
        {"filters": 64, "kernel_size": 4, "strides": 2, "padding": "same"},
        {"filters": 96, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 128, "kernel_size": 2, "strides": 2, "padding": "same"}
    ],
    "decoder": [
        {"filters": 128, "kernel_size": 2, "strides": 2, "padding": "same"},
        {"filters": 96, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 64, "kernel_size": 4, "strides": 2, "padding": "same"},
        {"filters": 64, "kernel_size": 5, "strides": 2, "padding": "same"},
        {"filters": 32, "kernel_size": 5, "strides": 2, "padding": "same"},
        {"filters": 32, "kernel_size": 5, "strides": 2, "padding": "same"}
    ],
    "default_activation": {"name": "leaky_relu", "alpha": 0.2},
    "embeddings_activation": {"name": "linear"},
    "output_activation": {"name" :  "sigmoid"},
    "optimizer": {"name": "Adam", "lr": 2e-4, "beta_1": 0.9, "beta_2": 0.99, "decay": 0.0},
    "reconstruction_loss": "bce",
    "metrics": ["mean_squared_error", "mean_absolute_error"]
}
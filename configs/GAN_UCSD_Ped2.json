{
    "input_shape": [128, 192, 1],
    "embeddings_size": 240,
    "embeddings_reshape": [2, 3],
    "use_resblock": "True",
    "use_spectral_norm": "True",
    "encoder": [
        {"filters": 32, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 32, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 64, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 64, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 96, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 128, "kernel_size": 3, "strides": 2, "padding": "same"}
    ],
    "decoder": [
        {"filters": 128, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 96, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 64, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 64, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 32, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 32, "kernel_size": 3, "strides": 2, "padding": "same"}
    ],
    "discriminator": [
        {"filters": 16, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 16, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 24, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 32, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 32, "kernel_size": 3, "strides": 2, "padding": "same"},
        {"filters": 32, "kernel_size": 3, "strides": 2, "padding": "same"}
    ],
    "default_activation": {"name": "leaky_relu", "alpha": 0.2},
    "embeddings_activation": {"name": "linear"},
    "optimizer": {"name": "rmsprop", "lr": 3e-4, "beta_1": 0.0, "beta_2": 0.99, "decay": 0.0, "rho": 0.9},
    "losses": {"discriminator": "bce", "autoencoder": "L1", "generator": "bce"},
    "metrics": {
        "discriminator": ["acc"],
        "autoencoder": ["mse"],
        "generator": ["acc"]
    }
}
{
    "model_architecture": "avp",
    "step_size": 8,
    "step_count": 2,
    "height": 128,
    "width": 128,
    "code_size": 256,
    "code_activation": "linear",
    "base_kernel_size": 3,
    "stem_kernel_size": 7,
    "basic_block_count": 2,
    "encoder": {
        "mode": "residual",
        "filters": [64, 96, 128, 128],
        "strides": [
            [2, 2, 2],
            [2, 2, 2],
            [1, 2, 2],
            [1, 2, 2]
        ]
    },
    "decoder": {
        "mode": "residual",
        "filters": [128, 128, 96, 64],
        "strides": [
            [1, 2, 2],
            [1, 2, 2],
            [2, 2, 2],
            [2, 2, 2]
        ]
    },
    "discriminator": {
        "mode": "residual",
        "filters": [64, 96, 128, 128],
        "strides": [
            [2, 2, 2],
            [2, 2, 2],
            [2, 2, 2],
            [1, 2, 2]
        ],
        "intermediate_size": 64
    },
    "output_activation": "tanh",
    "batch_size": 4,
    "learning_rate": 0.0005,
    "optimizer": "adam",
    "data_augmentation": {
        "dropout_noise_ratio": 0.0,
        "gaussian_noise_ratio": 0.0,
        "use_random_negative": false
    },
    "extract_patches": false,
    "seed": 42,
    "epochs": 256,
    "steps_per_epoch": 1000,
    "validation_steps": 128,
    "save_frequency": "epoch",
    "auc_frequency": -1,
    "led":
    {
        "goal": {
            "initial_rate": 0.2,
            "decay_steps": 1000,
            "decay_rate": 0.8,
            "staircase": false,
            "offset": 0.1
        }
    },
    "current_test": "AVP : Lowered recon, increased adversarial, reactivated GDL"
}
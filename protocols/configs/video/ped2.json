{
    "model_architecture": "minimalist_descriptor",
    "step_size": 8,
    "step_count": 4,
    "height": 32,
    "width": 32,
    "code_size": 16,
    "code_activation": "tanh",
    "kernel_size": 3,
    "encoder": {
        "filters": [8, 8, 16, 32, 32, 32],
        "strides": [
            [1, 2, 2],
            [1, 2, 2],
            [2, 1, 1],
            [1, 2, 2],
            [2, 1, 1],
            [1, 2, 2]
        ]
    },
    "decoder": {
        "filters": [32, 32, 32, 16, 8, 8],
        "strides": [
            [1, 2, 2],
            [1, 2, 2],
            [2, 1, 1],
            [1, 2, 2],
            [1, 2, 2],
            [2, 1, 1]
        ]
    },
    "discriminator": {
        "filters": [8, 16, 32, 64, 64],
        "strides": [
            [2, 2, 2],
            [1, 2, 2],
            [2, 2, 2],
            [1, 2, 2],
            [2, 2, 2]
        ],
        "intermediate_size": 32
    },
    "output_activation": "linear",
    "batch_size": 8,
    "learning_rate": 0.001,
    "data_augmentation": {
        "crop_ratio": 0.2,
        "dropout_noise_ratio": 0.0,
        "gaussian_noise_ratio": 0.0,
        "use_random_negative": false
    },
    "seed": 42,
    "epochs": 100,
    "steps_per_epoch": 1000,
    "validation_steps": 256
}
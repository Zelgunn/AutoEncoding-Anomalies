{
    "model_architecture": "minimalist_descriptor_v3",
    "step_size": 32,
    "step_count": 1,
    "height": 32,
    "width": 32,
    "code_size": 4,
    "code_activation": "relu",
    "kernel_size": 3,
    "basic_block_count": 2,
    "encoder": {
        "filters": [64, 64, 64, 64],
        "strides": [
            [2, 2, 2],
            [2, 2, 2],
            [2, 2, 2],
            [2, 2, 2]
        ]
    },
    "decoder": {
        "filters": [64, 64, 64, 64],
        "strides": [
            [2, 2, 2],
            [2, 2, 2],
            [2, 2, 2],
            [2, 2, 2]
        ]
    },
    "output_activation": "linear",
    "batch_size": 4,
    "learning_rate": 0.001,
    "data_augmentation": {
        "crop_ratio": 0.2,
        "dropout_noise_ratio": 0.0,
        "gaussian_noise_ratio": 0.0,
        "use_random_negative": false
    },
    "seed": 42,
    "epochs": 100,
    "steps_per_epoch": 1000,
    "validation_steps": 128
}
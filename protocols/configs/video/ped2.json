{
    "model_architecture": "led",
    "step_size": 32,
    "step_count": 1,
    "height": 128,
    "width": 128,
    "code_size": 128,
    "code_activation": "tanh",
    "kernel_size": 3,
    "basic_block_count": 2,
    "encoder": {
        "filters": [16, 24, 32, 64],
        "strides": [
            [2, 2, 2],
            [2, 2, 2],
            [2, 2, 2],
            [2, 2, 2]
        ]
    },
    "decoder": {
        "filters": [64, 32, 24, 16],
        "strides": [
            [2, 2, 2],
            [2, 2, 2],
            [2, 2, 2],
            [2, 2, 2]
        ]
    },
    "output_activation": "linear",
    "batch_size": 8,
    "learning_rate": 0.001,
    "data_augmentation": {
        "crop_ratio": 0.2,
        "dropout_noise_ratio": 0.0,
        "gaussian_noise_ratio": 0.0,
        "use_random_negative": false
    },
    "extract_patches": true,
    "seed": 42,
    "epochs": 10,
    "steps_per_epoch": 2000,
    "validation_steps": 128,
    "save_frequency" : "epoch"
}
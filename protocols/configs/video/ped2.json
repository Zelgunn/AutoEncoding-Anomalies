{
    "model_architecture": "bin_ae",
    "step_size": 32,
    "step_count": 1,
    "height": 128,
    "width": 128,
    "code_size": 128,
    "code_activation": "tanh",
    "kernel_size": 3,
    "basic_block_count": 2,
    "encoder": {
        "filters": [16, 32, 64, 128],
        "strides": [
            [2, 2, 2],
            [2, 2, 2],
            [2, 2, 2],
            [2, 2, 2]
        ]
    },
    "decoder": {
        "filters": [128, 64, 32, 16],
        "strides": [
            [2, 2, 2],
            [2, 2, 2],
            [2, 2, 2],
            [2, 2, 2]
        ]
    },
    "output_activation": "linear",
    "batch_size": 8,
    "learning_rate": 0.001,
    "data_augmentation": {
        "crop_ratio": 0.2,
        "dropout_noise_ratio": 0.0,
        "gaussian_noise_ratio": 0.0,
        "use_random_negative": false
    },
    "seed": 42,
    "epochs": 100,
    "steps_per_epoch": 4000,
    "validation_steps": 128,
    "save_frequency" : 100
}
{
    "model_architecture": "iae",
    "step_size": 8,
    "step_count": 8,
    "height": 128,
    "width": 128,
    "code_size": 128,
    "code_activation": "tanh",
    "kernel_size": [3, 5, 5],
    "encoder": {
        "filters": [16, 16, 64, 64, 128],
        "strides": [
            [1, 4, 4],
            [2, 1, 1],
            [1, 4, 4],
            [2, 1, 1],
            [1, 4, 4]
        ],
        "use_residual_bias": true,
        "use_conv_bias": false
    },
    "decoder": {
        "filters": [128, 64, 64, 16, 16],
        "strides": [
            [1, 4, 4],
            [2, 1, 1],
            [1, 4, 4],
            [2, 1, 1],
            [1, 4, 4]
        ],
        "use_residual_bias": true,
        "use_conv_bias": false
    },
    "output_activation": "linear",
    "use_batch_norm": false,
    "batch_size": 32,
    "learning_rate": 1e-4,
    "data_augmentation": {
        "crop_ratio": 0.1,
        "dropout_noise_ratio": 0.0,
        "gaussian_noise_ratio": 0.0,
        "use_random_negative": false
    },
    "discriminator": {
        "filters": [16, 32, 64, 128, 128],
        "strides": [
            [1, 2, 2],
            [1, 2, 2],
            [1, 2, 2],
            [1, 2, 2],
            [1, 2, 2]
        ],
        "intermediate_size": 128
    }
}